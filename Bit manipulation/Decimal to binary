def main():
    # YOUR CODE GOES HERE
    # Please take input and print output to standard input/output (stdin/stdout)
    # E.g. 'input()/raw_input()' for input & 'print' for output

    T = int(input())
    for i in range(T):
    
        number = int(input())
        ans = 0
        power = 1
        while number>0:
            remainder = number%2
            ans += remainder*power
            power*=10
            number//=2

        print(ans) 


if __name__ == '__main__':
    main()
